#!/usr/bin/env python3
"""
RAG project command-line script.
This script allows you to run the RAG project's CLI without installing the package.
"""

import sys
import os

# Add the project root directory to the Python path
sys.path.insert(0, os.path.dirname(os.path.abspath(__file__)))

# Import and run the CLI main function
from rag_project.cli.main import main

if __name__ == "__main__":
    main()